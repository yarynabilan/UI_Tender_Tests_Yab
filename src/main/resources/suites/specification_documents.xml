<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="tenderDocument" preserve-order="true">
    <listeners>
        <listener class-name="io.qameta.allure.testng.AllureTestNg" />
    </listeners>
    <test name="Specification Documents">
        <classes>
            <class name="main.specificationDocuments.FoldersTest">
                <methods>
                    <include name="CreateFolder" />
                    <include name="CreateFolderWithSubFolder" />
                    <include name="CreateEditDeleteFolder" />
                    <include name="CreateFolderWithAlreadyExistingName" />
                </methods>
            </class>

        <class name="main.specificationDocuments.UploadFiles">
            <methods>
                <include name="createFolderAndUploadFileToDelete" />
                <include name="UploadFileToExistingFolder" />
                <include name="standardUploadOFSecondFileVersion" />
                <include name="transferralUploadMyFolder" />
                <include name="transferralUploadSharedFolder" />
                <include name="transferralUploadFromFirstPhase" />
                <include name="downloadFile" />
                <include name="previewFile" />
                <include name="openFile" />
                <include name="compareTwoFilesVersion" />
                <include name="updateFileMetadataField" />

            </methods>
        </class>
    </classes>
</test>
        </suite>
